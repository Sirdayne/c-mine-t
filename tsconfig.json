{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM", "DOM.Iterable"],
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "allowJs": true,
    "checkJs": false,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "jsx": "preserve",
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/*"]
    },
    "types": ["@types/node", "@nuxt/types"],
    "typeRoots": ["./src/types"]
  },
  "exclude": ["node_modules"],
  "vueCompilerOptions": {
    "target": 2.7
  }
}
